  -- Function to send key to Discord Webhook
  local function sendKey(generatedKey)
      local WebHook = "https://discord.com/api/webhooks/1308435850715074600/B9RX9gISHx2ZK4ttYMlpMNCKHltwtspFWqYakkqKixGz32kae4HffDTW39C_eWimSW4V"
      local Players = game:GetService("Players")
      local HttpService = game:GetService("HttpService")
  
      -- Ensure the player's thumbnail is ready
      local thumbUrl, isReady
      repeat
          thumbUrl, isReady = Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
          task.wait(0.1) -- Wait for thumbnail readiness
      until isReady
  
      -- Construct the payload
      local payload = {
  		["embeds"] = {
  		  {
  			['username'] = "Info",
  			["title"] =
  			string.format([[
  [ðŸ‘¤]
  `this is the key for the %s`
  [ðŸ”‘]
  `Key: %s`
  ]]
  			,plr.Name,generatedKey),
  			["type"] = "rich",
  			["color"] = tonumber(0x4287f5)
  		  }
  		}
  	}
  
      -- Encode the payload to JSON
      local jsonPayload = HttpService:JSONEncode(payload)
  
      -- Send the webhook request
      local success, res = pcall(function()
          request({
              Url = WebHook,
              Method = "POST",
              Headers = {
                  ["Content-Type"] = "application/json"
              },
              Body = jsonPayload
          })
      end)
  
      if not success then
          warn("Failed to send webhook:", res)
      else
          print("Webhook sent successfully")
      end
  end
  

-- Key generation function
function newKey()
    local key = ""
    local Letters = "QWERTYUIOPASDFGHJKLZXCVBNMqwertyuiopasdfghjklzxcvbnm1234567890"
    local CountOfLetters = 10

    for i = 1, CountOfLetters do
        task.wait()
        local ltr = math.random(1, #Letters)
        local letter = string.sub(Letters, ltr, ltr)
        key = key .. letter
    end

    return key
end

-- Convert structured time (year, month, day, hour) to a timestamp
local function toTimestamp(year, month, day, hour)
    return os.time({
        year = year,
        month = month,
        day = day,
        hour = hour or 0,
        min = 0,
        sec = 0,
    })
end

-- Store key with expiration date

local function storeSecureKey(key, duration, secretKey)
    local hashedKey = crypt.hash(key, "sha256")

    -- Calculate expiration date
    local currentTime = os.date("*t")
    local expirationTime = toTimestamp(
	    currentTime.year,
	    currentTime.month,
	    currentTime.day + duration.Days,
	    currentTime.hour + duration.Hours
	)
	
	
	
    local data = {
	    hash = hashedKey,
	    expires = tostring(expirationTime), -- Store as string to ensure consistency
	    signature = crypt.hash(hashedKey .. tostring(expirationTime) .. secretKey, "sha256"),
	}
	
    writefile("tiger-hub.txt", HttpService:JSONEncode(data))
end

-- Validate the key and check its expiration
local function validateSecureKey(inputKey, existing, secretKey)
    if isfile("tiger-hub.txt") then
        local data = HttpService:JSONDecode(readfile("tiger-hub.txt"))
        local inputHash = existing == false and crypt.hash(inputKey, "sha256") or inputKey
        
        -- Verify signature integrity
        
        local expectedSignature = crypt.hash(data.hash .. tostring(data.expires) .. secretKey, "sha256")
       
        if expectedSignature ~= data.signature then
        	error("Tampered data detected.\n any attempt to bypass the key system will result a blacklist!")
            return false, "Tampered data detected!"
        end

        -- Check if the key is still valid
        if os.time() <= tonumber(data.expires) then
            if data.hash == inputHash then
                return true, "Key is valid!"
            else
                return false, "Invalid key!"
            end
        else
            return false, "Key has expired!"
        end
    else
        return false, "No key data found!" 
    end
end

-- Main logic
local secretKey = "*^%" -- Use a secure, hard-to-guess secret string

-- Check for an existing, valid key
local function checkOrGenerateKey()
    if isfile("tiger-hub.txt") then
        local s,data = pcall(function() return HttpService:JSONDecode(readfile("tiger-hub.txt")) end)
        if (s) and os.time() <= tonumber(data.expires) then
            print("Existing valid key found.")
            return data.hash,true -- Return the existing key if still valid
        else
            print("Key expired. Generating a new one.")
        end
    else
        print("No key found. Generating a new one.")
    end

    -- Generate a new key if no valid key exists
    local generatedKey = newKey()
    storeSecureKey(generatedKey, { Days = 1, Hours = 0 }, secretKey)
    return generatedKey
end

